<launch>
  <arg name="ip" default='"10.42.0.1"'/>
  <arg name="config_file" default="config/config_crosby_nxp.json"/>
  <arg name="mode" default="1"/>
  <arg name="use_depthCompute" default="true"/>
  <node pkg="aditof_roscpp" type="aditof_camera_node" name="aditof_camera_node" args="ip=$(arg ip) config_file=$(arg config_file) mode=$(arg mode) use_depthCompute=$(arg use_depthCompute) rqt=false" output="screen" cwd='ROS_HOME'/>
</launch>
  